<!--packageA/pages/index/index.wxml-->
<scroll-view scroll-y class="scrollPage">
  <view class='nav-list' style="padding: 0;">
      <navigator hover-class='none' url="packageA/fleetManage/setting/index" style="padding: 100rpx;border-top-left-radius: 0;border-top-right-radius: 0;width: 100%; margin:0;background-image:url(https: //image.weilanwl.com/color2.0/cardBg.png)" class="nav-li bg-blue" >
        <view class="nav-title">车队管理系统</view>
        <view class="nav-name">Manage</view>
        <text bindtap="setting" class='cuIcon-newsfill'></text>
      </navigator>
  </view>
  <view>
      <view class="padding-sm margin-top" bindtap="audited">
        <view class="bg-gradual-green padding radius text-center shadow-blur" style="padding:80rpx;">
          <view class="text-lg">分配</view>
        </view>
      </view>
      <view class="padding-sm" bindtap="unAudited">
        <view class="bg-gradual-blue padding radius text-center shadow-blur" style="padding:80rpx;">
          <view class="text-lg">充值</view>
        </view>
      </view>
  </view>
  <view class="box">
    <view class="cu-bar tabbar bg-white">
      <view class="action">
        <view class="cuIcon-cu-image">
          <image src="../../images/tabbar/basics_cur.png"></image>
        </view>
        <view class="text-green">首页</view>
      </view>
      <view class="action" bindtap="goUser">
        <view class="cuIcon-cu-image">
          <image src="../../images/tabbar/about.png"></image>
          <view class="cu-tag badge"></view>
        </view>
        <view class="text-gray">我的</view>
      </view>
    </view>
  </view>

  <view class="cu-tabbar-height"></view>
</scroll-view>
