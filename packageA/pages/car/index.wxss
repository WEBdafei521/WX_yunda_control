/* pages/tool/myCar/index.wxss */
page{
  background: #F4F3F3;
}
.borders{
  border-bottom:1px solid rgba(50,50,50,.2);
}
.jumu{
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  z-index: 10;
  background: rgba(100,100,100,.8);
}
.hide{
  display: none;
}
.aaa{
  position: fixed;
  width:100vw;height:500px;background:#fff;bottom: -500px;
  border-top-left-radius: 8px;
  border-top-right-radius: 8px;
  transition: all .5s linear;
  z-index: 100;
}
.selected{
 bottom: 0px;
}
 .inputs{
  margin: 8px;
  padding: 8px;
  padding-left: 20px;
  border-bottom: 1px solid rgba(100, 100,100,.3)
}

.add{
  width: 120px;
  height: 40px;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  border: 1px solid #1888F6;
}

